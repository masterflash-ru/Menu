{
    "name": "masterflash-ru/menu",
    "description": "Генератор меню",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "simba"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "zendframework/zend-i18n": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Menu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Menu\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.0-dev"
        },
        "zf": {
            "component": "Menu",
            "config-provider": "masterflash-ru\\Menu\\ConfigProvider"
        }
    }
}
